{
  "name": "ng-catbee",
  "version": "20.3.0",
  "description": "Angular libraries",
  "keywords": [
    "angular"
  ],
  "author": "Hari G <hprasath.dev@gmail.com>",
  "scripts": {
    "build": "node scripts/build-all.mjs",
    "build:utils": "cd packages/utils && npm run build",
    "build:monaco-editor": "cd packages/monaco-editor && npm run build",
    "build:loader": "cd packages/loader && npm run build",
    "build:jwt": "cd packages/jwt && npm run build",
    "build:storage": "cd packages/storage && npm run build",
    "build:cookie": "cd packages/cookie && npm run build",
    "build:indexed-db": "cd packages/indexed-db && npm run build",
    "lint": "ng lint",
    "prelint:fix": "npm run format",
    "lint:fix": "ng lint --fix",
    "test-base": "ng t --no-progress --code-coverage --no-watch --browsers=ChromeHeadless",
    "pretest": "rimraf coverage",
    "test": "npx npm-run-all test:*",
    "posttest": "npm run coverage:merge && npm run coverage:overall",
    "test:utils": "cross-env COVERAGE_DIR=utils npm run test-base -- utils",
    "test:monaco-editor": "cross-env COVERAGE_DIR=monaco-editor npm run test-base -- monaco-editor",
    "test:loader": "cross-env COVERAGE_DIR=loader npm run test-base -- loader",
    "test:jwt": "cross-env COVERAGE_DIR=jwt npm run test-base -- jwt",
    "test:storage": "cross-env COVERAGE_DIR=storage npm run test-base -- storage",
    "test:cookie": "cross-env COVERAGE_DIR=cookie npm run test-base -- cookie",
    "test:indexed-db": "cross-env COVERAGE_DIR=indexed-db npm run test-base -- indexed-db",
    "coverage:merge": "node scripts/coverage-merge.mjs",
    "coverage:overall": "node scripts/coverage-generator.mjs",
    "coverage:nyc": "nyc report  --reporter=text --reporter=text-summary --reporter=lcovonly --reporter=cobertura --reporter=html --report-dir=coverage  -t coverage",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{ts,html,scss,css}\" \"!**/*.min.css\"",
    "release": "node scripts/npm-release.mjs",
    "release:utils": "node scripts/npm-release.mjs utils",
    "release:monaco-editor": "node scripts/npm-release.mjs monaco-editor",
    "release:loader": "node scripts/npm-release.mjs loader",
    "release:jwt": "node scripts/npm-release.mjs jwt",
    "release:storage": "node scripts/npm-release.mjs storage",
    "release:cookie": "node scripts/npm-release.mjs cookie",
    "release:indexed-db": "node scripts/npm-release.mjs indexed-db",
    "deps:update": "npx npm-check-updates -u --target latest && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "^21.0.1",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@eslint/js": "^9.39.1",
    "@ng-catbee/monaco-editor": "^20.1.3",
    "angular-eslint": "^21.0.1",
    "eslint": "^9.39.1",
    "monaco-editor": "^0.55.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.48.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jasmine": "~5.1.13",
    "@types/karma-chrome-launcher": "^3.1.4",
    "@types/karma-coverage": "^2.0.3",
    "@types/karma-jasmine": "^4.0.5",
    "@types/karma-jasmine-html-reporter": "^1.7.3",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.2.0",
    "jasmine-core": "~5.12.1",
    "jsdom": "^27.2.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^21.0.0",
    "npm-run-all2": "^8.0.4",
    "nyc": "^17.1.0",
    "prettier": "^3.7.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.14"
  }
}
