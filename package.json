{
  "name": "ng-catbee",
  "version": "20.3.0",
  "description": "Angular libraries",
  "keywords": [
    "angular"
  ],
  "author": "Hari G <hprasath.dev@gmail.com>",
  "scripts": {
    "test": "ng t --no-progress --code-coverage --no-watch --browsers=ChromeHeadless",
    "lint": "ng lint",
    "build": "node scripts/build-all.mjs",
    "build:monaco-editor": "cd packages/monaco-editor && npm run build",
    "build:utils": "cd packages/utils && npm run build",
    "lint:fix": "ng lint --fix",
    "prelint:fix": "npm run format",
    "prepare": "husky",
    "format": "prettier --write **/*.{ts,html,scss,css}",
    "release:utils": "node scripts/npm-release.mjs utils",
    "release:monaco-editor": "node scripts/npm-release.mjs monaco-editor",
    "deps:update": "npx npm-check-updates -u --target latest && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "^21.0.1",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@eslint/js": "^9.39.1",
    "@ng-catbee/monaco-editor": "^20.1.3",
    "angular-eslint": "^21.0.1",
    "eslint": "^9.39.1",
    "monaco-editor": "^0.55.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.48.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jasmine": "~5.1.13",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^21.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3"
  }
}
